scalar Upload

extend type Query {
	getMyKYBSetting : KYB
    getKYBSetting(
		userId: String!
	) : KYB
    getKYBSettingList : [KYB]
}

extend type Mutation {
    updateInfo (
        country: String!,
        companyName: String!,
        regNum: String!
    ) : KYB
    updateFile (
        files: [Upload]!
    ) : KYB
}

schema {
    query: Query,
    mutation: Mutation
}