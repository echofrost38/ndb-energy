type Mutation {
	placeBid(
		userId: String!,
		roundNumber: Int!,
		tokenAmount: Float!,
		tokenPrice: Float!,
		payType: String!
	) : Bid	
	updateBid(
		userId: String!,
		roundNumber: Int!,
		tokenAmount: Float!,
		tokenPrice: Float!
	) : Bid
	createAuction(
		number: Int!, 
		startedAt: String!,  
		totalToken: Float!, 
		minPrice: Float!
	) : Auction!
	updateAuction(
		id: String!
		number: Int, 
		startedAt: String,  
		totalToken: Float, 
		minPrice: Float		
	) : Auction!
}