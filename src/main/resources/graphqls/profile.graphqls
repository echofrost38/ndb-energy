scalar Upload

extend type Mutation {
	setAvatar(
		prefix: String!,
		name: String!
	) : String
	updateAvatarSet(
		components: [AvatarSetInput]
	) : [AvatarSet]
	updateKYCSetting(
		kind: String!,
		bid: Float,
		direct: Float,
		deposit: Float,
		withdraw: Float
	) : Int
	verifyKYC(
		shuftiRequest: ShuftiRequestInput
	) : Int
}

extend type Query {
	getUserTierTask: TierTask
	getKYCSettings : [KYCSetting]
	kycStatusRequest : Int
}

schema {
	mutation: Mutation,
	query: Query,
}
