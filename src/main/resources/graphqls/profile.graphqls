scalar Upload

extend type Mutation {
	setAvatar(
		prefix: String!,
		name: String!
	) : String
	updateAvatarSet(
		components: [AvatarSetInput]
	) : [AvatarSet]
	initVerification(
		country: String!,
		docType: String!
	) : String
	uploadDocument(
		file: [Upload]
	) : String	
	uploadSelfie(
		selfie: Upload
	) : String
	requestCheck : String
}

schema {
	mutation: Mutation
}