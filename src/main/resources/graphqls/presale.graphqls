extend type Mutation {
    createNewPresale(
        round: Int!,
        startedAt: Float!,
        endedAt: Float!,
        tokenAmount: Float!,
        tokenPrice: Float!,
        conditions: [PreSaleConditionInput!]!
    ) : Int
}

extend type Query {
    getPreSales: [PreSale]
    getPreSaleByStatus(
        status: Int
    ) : [PreSale]
}

schema {
    mutation: Mutation,
    query: Query
}