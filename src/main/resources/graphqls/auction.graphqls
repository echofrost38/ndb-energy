extend type Query {
	checkRounds: String
	getAuctions: [Auction]
	getAuctionByStatus(
		status: Int!
	) : [Auction]
	getAuctionById(id: Int!) : Auction
	getAuctionByNumber(
		round: Int!
	) : Auction
}

extend type Mutation {
	createAuction(
		round: Int!, 
		startedAt: String!,  
		duration: Float!,
		totalToken: Float!, 
		minPrice: Float!,
		avatar: [AvatarSetInput],
		token: Float
	) : Auction
	updateAuction(
		id: String!
		round: Int, 
		duration: Float,  
		totalToken: Float, 
		minPrice: Float,
		avatar: [AvatarSetInput],
		token: Float
	) : Auction!
}

schema {
   query: Query,
   mutation: Mutation
}