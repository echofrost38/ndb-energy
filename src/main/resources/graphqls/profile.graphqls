scalar Upload

extend type Mutation {
	setAvatar(
		prefix: String!,
		name: String!
	) : String
	updateAvatarSet(
		components: [AvatarSetInput]
	) : [AvatarSet]
	createApplicant(
		country: String!,
		docType: String!,
		levelName: String!
	) : String
	upload(
		docType: String!
		file: Upload!
	) : String	
	uploadSelfie(
		selfie: Upload
	) : String
	requestCheck : String
}

extend type Query {
	gettingApplicantData(
		applicantId: String!
	) : String
}

schema {
	mutation: Mutation,
	query: Query
}