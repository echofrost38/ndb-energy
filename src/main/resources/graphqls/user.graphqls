extend type Query {
	getUser : User
    getUserCount : Int
    getPaginatedUsers(
        key: String,
        limit: Int!
    ) : [User]
}

extend type Mutation {
    addDisallowed(
        countryCode: String!
    ) : GeoLocation
    getDisallowed :
        [GeoLocation]
    makeAllow(
        countryCode: String!
    ) : GeoLocation
    resetPasswordByAdmin(
        email: String!
    ) : String
    createNewUser(
        email: String!,
        country: String!,
        role: String!,
        avatarName: String!,
        shortName: String!
    ) : String
    changeRole(
        email: String!,
        role: String!
    ) : String
    changePassword(
        newPassword: String!
    ) : String!
}

schema {
    query: Query,
    mutation: Mutation
}