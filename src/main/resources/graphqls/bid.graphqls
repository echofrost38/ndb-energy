type Mutation {
	placeBid(
		userId: String!,
		roundId: String!,
		tokenAmount: Float!,
		tokenPrice: Float!,
		payType: Int!,
		cryptoType: String
	) : Bid	
	updateBid(
		userId: String!,
		roundId: String!,
		tokenAmount: Float!,
		tokenPrice: Float!
	) : Bid
	increaseBid(
		userId: String!,
		roundId: String!,
		tokenAmount: Float!,
		tokenPrice: Float!,
		payType: Int!,
		cryptoType: String
	) : Bid
}

type Query {
	getAuctions : [Auction]
	getAuctionById(id: String!) : Auction!
}