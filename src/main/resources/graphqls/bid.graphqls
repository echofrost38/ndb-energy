type Mutation {
	placeBid(
		userId: String!,
		roundNumber: Int!,
		tokenAmount: Float!,
		tokenPrice: Float!,
		payType: Int!
	) : Bid	
	updateBid(
		userId: String!,
		roundNumber: Int!,
		tokenAmount: Float!,
		tokenPrice: Float!
	) : Bid
}

type Query {
	getAuctions : [Auction]
	getAuctionById(id: String!) : Auction!
}